{
  "app": "python3 app.py",
  "watch": {
    "include": [
      "**"
    ],
    "exclude": [
      "README.md",
      "cdk*.json",
      "requirements*.txt",
      "source.bat",
      "**/__init__.py",
      "python/__pycache__",
      "tests"
    ]
  },
  "context": {
    "EnableDebug": "True",
    "PermissionsBoundaryPolicyArn": "arn:aws:iam::aws:policy/AdministratorAccess",
    "KmsKeyAlias": "alias/aws/s3"
  }
}
